MODULE ZSET_ADD_HEAD_01 INPUT.
DATA ZSD0303 TYPE char30.
IF VBAK-ZSD0303  IS NOT INITIAL.
SELECT SINGLE ZSD00202
     FROM ZSD002
     INTO ZSD0303
     WHERE ZSD00201 = VBAK-ZSD0303
             and SPRAS = sy-langu..
ENDIF.


ENDMODULE.

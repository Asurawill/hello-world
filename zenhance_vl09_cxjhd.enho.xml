   <TOOL>HOOK_IMPL</TOOL>
   <SHORTTEXT>交货单冲销检查</SHORTTEXT>
   <ORIGINAL_OBJECT>
    <PGMID>R3TR</PGMID>
    <ORG_OBJ_TYPE>PROG</ORG_OBJ_TYPE>
    <ORG_OBJ_NAME>SAPMV50A</ORG_OBJ_NAME>
